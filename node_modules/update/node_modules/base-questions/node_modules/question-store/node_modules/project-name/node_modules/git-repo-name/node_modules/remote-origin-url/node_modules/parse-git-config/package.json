{
  "name": "parse-git-config",
  "description": "Parse `.git/config` into a JavaScript object. sync or async.",
  "version": "0.4.2",
  "homepage": "https://github.com/jonschlinkert/parse-git-config",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "repository": "jonschlinkert/parse-git-config",
  "bugs": {
    "url": "https://github.com/jonschlinkert/parse-git-config/issues"
  },
  "license": "MIT",
  "files": [
    "index.js"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "test": "mocha"
  },
  "dependencies": {
    "extend-shallow": "^2.0.1",
    "git-config-path": "^0.2.0",
    "ini": "^1.3.4"
  },
  "devDependencies": {
    "gulp-format-md": "^0.1.7",
    "mocha": "^2.4.5",
    "os-homedir": "^1.0.1"
  },
  "keywords": [
    "config",
    "git",
    "parse"
  ],
  "verb": {
    "run": true,
    "toc": false,
    "layout": "default",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "related": {
      "list": [
        "parse-github-url",
        "parse-gitignore",
        "parse-author",
        "parse-authors",
        "git-user-name",
        "git-username"
      ]
    },
    "reflinks": [
      "verb"
    ],
    "lint": {
      "reflinks": true
    }
  }
}
